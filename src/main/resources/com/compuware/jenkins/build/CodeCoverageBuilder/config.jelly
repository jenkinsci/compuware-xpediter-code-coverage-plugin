<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form" xmlns:c="/lib/credentials">

      <f:entry title="${%hostConnection}" field="connectionId">
            <f:select default=""/>
      </f:entry>
      
      <f:entry title="${%loginCredentials}" field="credentialsId">
            <c:select default=""/>
      </f:entry>
 
      <f:entry title="${%analysisPropertiesPath}" field="analysisPropertiesPath">
            <f:textbox default="${descriptor.defaultAnalysisPropertiesPath}"/>
      </f:entry>
      
      <f:entry title="${%analysisProperties}" field="analysisProperties">
            <f:textarea default="${descriptor.defaultAnalysisProperties}"/>
      </f:entry>

</j:jelly>
